{
  "name": "rmc-software-documentation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "counters": "node scripts/counters.js",
    "start": "npm run counters && docusaurus start",
    "build:docusaurus": "npm run counters && docusaurus build",
    "convert:pdf": "node scripts/postBuild.js",
    "build": "npm run build:docusaurus && npm run convert:pdf",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build:pdf": "node renderReactToHTML.js && pandoc ./output/html/*.html -o ./static/pdfs/output.pdf --pdf-engine=xelatex"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/plugin-google-analytics": "^3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-classic": "^3.7.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "gh-pages": "^6.3.0",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.0",
    "markdown-it-cite": "^0.0.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "child_process": "^1.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
